\pard\sl276\slmult1\b0 Cvi\'e8en\f1\lang1033\'ed\f0\lang1029 : kalkul\f1\lang1033\'e1\f0\lang1029 tor zpropitn\f1\lang1033\'e9\f0\lang1029 ho\par
\par
\f1\lang1033\tab\b print("Welcome to the tip calculator.")\par

\pard\li720\sl276\slmult1 total = float(input("Zadej celkovou utratu (i s $ na zacatku): ")[1:])\par
tip_percent = int(input("Zadej velikost zpropitneho v procentech (10, 12 nebo 15): "))\par
